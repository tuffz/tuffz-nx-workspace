{
  "name": "tuffz-info-e2e",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "sourceRoot": "apps/tuffz-info-e2e/src",
  "targets": {
    "e2e": {
      "executor": "@nx/cypress:cypress",
      "options": {
        "cypressConfig": "apps/tuffz-info-e2e/cypress.config.ts",
        "testingType": "e2e",
        "devServerTarget": "tuffz-info:serve"
      },
      "configurations": {
        "production": {
          "devServerTarget": "tuffz-info:serve:production"
        },
        "ci": {
          "devServerTarget": "tuffz-info:serve-static"
        }
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["apps/tuffz-info-e2e/**/*.{js,ts}"]
      }
    },
    "version": {
      "executor": "@jscutlery/semver:version",
      "options": {
        "preset": "conventional"
      }
    }
  },
  "implicitDependencies": ["tuffz-info"],
  "tags": []
}
