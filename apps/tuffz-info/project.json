{
  "name": "tuffz-info",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/tuffz-info/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nx/webpack:webpack",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "production",
      "options": {
        "compiler": "babel",
        "outputPath": "dist/apps/tuffz-info",
        "index": "apps/tuffz-info/src/index.html",
        "baseHref": "/",
        "main": "apps/tuffz-info/src/main.tsx",
        "tsConfig": "apps/tuffz-info/tsconfig.app.json",
        "assets": [
          "apps/tuffz-info/src/favicon.ico",
          "apps/tuffz-info/src/assets"
        ],
        "styles": ["apps/tuffz-info/src/styles.scss"],
        "scripts": [],
        "isolatedConfig": true,
        "postcssConfig": "apps/tuffz-info/postcss.config.js",
        "webpackConfig": "apps/tuffz-info/webpack.config.js"
      },
      "configurations": {
        "development": {
          "extractLicenses": false,
          "optimization": false,
          "sourceMap": true,
          "vendorChunk": true
        },
        "production": {
          "fileReplacements": [
            {
              "replace": "apps/tuffz-info/src/environments/environment.ts",
              "with": "apps/tuffz-info/src/environments/environment.prod.ts"
            }
          ],
          "optimization": true,
          "outputHashing": "all",
          "sourceMap": false,
          "namedChunks": false,
          "extractLicenses": true,
          "vendorChunk": false
        }
      }
    },
    "serve": {
      "executor": "@nx/webpack:dev-server",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "tuffz-info:build",
        "hmr": true
      },
      "configurations": {
        "development": {
          "buildTarget": "tuffz-info:build:development"
        },
        "production": {
          "buildTarget": "tuffz-info:build:production",
          "hmr": false
        }
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["apps/tuffz-info/**/*.{ts,tsx,js,jsx}"]
      }
    },
    "serve-static": {
      "executor": "@nx/web:file-server",
      "options": {
        "buildTarget": "tuffz-info:build"
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "apps/tuffz-info/jest.config.ts"
      }
    },
    "version": {
      "executor": "@jscutlery/semver:version",
      "options": {
        "preset": "conventional"
      }
    }
  },
  "tags": ["tuffz-info"]
}
